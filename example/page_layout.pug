doctype html
html
    head
        meta(charset="UTF-8")
        meta(name="viewport",
            content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0")
        meta(http-equiv="X-UA-Compatible", content="ie=edge")
        block title
        link(rel="preconnect", href="https://cdn.jsdelivr.net", crossorigin="anonymous")
        link(rel="preload",
            as="style",
            crossorigin,
            href="https://cdn.jsdelivr.net/gh/orioncactus/pretendard@v1.3.9/dist/web/variable/pretendardvariable-dynamic-subset.css")
        link(rel="stylesheet",
            href="https://cdn.jsdelivr.net/gh/orioncactus/pretendard@v1.3.9/dist/web/variable/pretendardvariable-dynamic-subset.css")
        link(href="../style.css", rel="stylesheet")
        link(href="../frogui.css", rel="stylesheet")
        link(href="../../prism/prism.css", rel="stylesheet")

        //- link(href="../style.css", rel="stylesheet")
    body.pages
        .side-menu
            .background
            .menu-wrapper
                - var path = '..'
                include lnb.pug
        header.gnb
            nav.main-menu
                .toggle-menu
                    .ga-button.icon.circle.show-menu
                        i.fa.fa-bars
            nav.sub-menu.float
                .home 
                    a.ga-button.icon.circle(href="../index.html")
                        i.fa.fa-home
                .mode
                    .ga-button.icon.circle.toggle-mode
                        i.fa.fa-sun
        .main-container
            .left-part 
                - var path = '..'
                    include lnb.pug
            .right-part
                .container
                    block contents
        script(src="../../prism/prism.js")
        script(src="../js/base.js", type="text/javascript")
        script(src="../js/frog_ui.js", type="text/javascript")
        block module