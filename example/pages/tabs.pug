        h4.contents-title Browser Tab
        .ga-card.attached
            .contents-wrapper
                .ga-tabs.browser
                    - var tabMenu = [1, 2, 3, 4, 5]
                    .tab-menu
                        each tab in tabMenu
                            .tab-item.tab
                                span.tabText.ellipsis=`Item ${tab}`
                                .btn-close.ga-button.icon.tiny.circle
                                    i.fa.fa-close
                    .tab-contents
                        .tab-plus.ga-button.icon.primary ADD
                            i.icon.right.fa.fa-plus
            .footer
                - var code_1_s = `<div class="ga-tabs browser">`
                - var code_2_s = `      <div class="tab-menu">`
                - var code_c_1 = `          <div class="tab-item tab active">\n             <span class="tabText ellipsis">Item 1</span>\n             <span class="btn-close"><i class="fa fa-colse" /></span>\n          </div>`
                - var code_c_2 = `          <div class="tab-item tab">\n            <span class="tabText ellipsis">Item 2</span>\n            <span class="btn-close"><i class="fa fa-colse" /></span>\n          </div>`
                - var code_e = `            ...`
                - var code_c_3 = `          <div class="tab-item tab">\n            <span class="tabText ellipsis">Item N</span>\n            <span class="btn-close"><i class="fa fa-colse" /></span>\n          </div>`
                - var code_2_f = `      </div>`
                - var code_1_f = `  </div>`
