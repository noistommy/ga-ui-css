section#tabs
    .section-title
        h1 Tabs
    .section-contents
        h4.contents-title 기본
        .ga-card.attached
            .contents-wrapper
                .ga-tabs
                    .tab-menu
                        .tab-item.tab.active Item 1
                        .tab-item.tab Item 2
                        .tab-item.tab Item 3
                        .tab-item.tab Item 4
                        .tab-item.tab Item 5
            .footer
                - var code_1_s = `<div class="ga-tabs">`
                - var code_2_s = `      <div class="tab-menu">`
                - var code_c_1 = `          <div class="tab-item tab active">Item 1</div>`
                - var code_c_2 = `          <div class="tab-item tab">Item 2</div>`
                - var code_e = `            ...`
                - var code_c_3 = `          <div class="tab-item tab">Item n</div>`
                - var code_2_f = `      </div>`
                - var code_1_f = `  </div>`
                pre 
                    code.language-markup 
                        | #{code_1_s}
                        | #{code_2_s}
                        | #{code_c_1}
                        | #{code_c_2}
                        | #{code_e}
                        | #{code_c_3}
                        | #{code_2_f}
                        | #{code_1_f}
